<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<smartbi>
<framework>
<log-config>log4j.rootLogger=INFO, R, C
log4j.logger.org.hibernate=ERROR
log4j.logger.org.quartz=ERROR
log4j.logger.smartbi=DEBUG
log4j.appender.R=org.apache.log4j.RollingFileAppender
log4j.appender.R.File=/tomcat/smartbiconfig/smartbi_config/smartbi.log
log4j.appender.R.MaxFileSize=10240KB
log4j.appender.R.MaxBackupIndex=5
log4j.appender.R.layout=org.apache.log4j.PatternLayout
log4j.appender.R.layout.ConversionPattern=%d{MM-dd HH:mm:ss} %5p %M(%c:%L) - %m%n
log4j.appender.C=org.apache.log4j.ConsoleAppender
log4j.appender.C.layout=org.apache.log4j.PatternLayout
log4j.appender.C.layout.ConversionPattern=%d{MM-dd HH:mm:ss} %5p %M(%c:%L) - %m%n
</log-config>
<license-file>/tomcat/smartbiconfig/smartbi_config</license-file>
<skip-log4j-handler>true</skip-log4j-handler>
</framework>
